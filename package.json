    "@fortawesome/free-solid-svg-icons": "6.3",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@hookform/resolvers": "^2.9.11",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.12.0",
    "@trpc/react-query": "^10.12.0",
    "@trpc/server": "^10.0.0",
    "@vercel/analytics": "^0.1.10",
    "ably": "^1.2.36",
    "aws-sdk": "^2.1323.0",
    "classnames": "^2.3.2",
    "daisyui": "^2.51.1",
    "encoding": "^0.1.13",
    "ethers": "^5.7.2",
    "lokijs": "^1.5.12",
    "multer": "^1.4.5-lts.1",
    "next": "^13.2.1",
    "next-auth": "^4.19.2",
    "next-connect": "^0.13.0",
    "next-superjson": "^0.0.4",
    "pino-pretty": "^9.4.0",
    "purify-ts": "^1.3.3",
    "react": "^18.2.0",
    "react-daisyui": "^3.0.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-markdown": "^8.0.5",
    "siwe": "^2.1.3",
    "superjson": "1.12",
    "wagmi": "^0.11.7",
    "zod": "^3.20.6"
