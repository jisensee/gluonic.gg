    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "@vercel/analytics": "^0.1.5",
    "ably": "^1.2.34",
    "aws-sdk": "^2.1267.0",
    "classnames": "^2.3.2",
    "daisyui": "^2.42.1",
    "ethers": "^5.5.1",
    "multer": "^1.4.5-lts.1",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "next-connect": "^0.13.0",
    "next-pwa": "^5.6.0",
    "next-superjson": "^0.0.4",
    "purify-ts": "^1.3.0",
