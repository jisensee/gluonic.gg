  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next",
    "codegen-dev": "graphql-codegen && prisma generate",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "check": "tsc && npm run lint",
    "build": "next build",
    "start": "next start",
    "vercel-build": "npm run check && npm run prisma:generate-prod && npm run prisma:migrate-prod && next build",
    "prisma:migrate-prod": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "prisma:generate-prod": "prisma generate --data-proxy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@hookform/resolvers": "^2.9.8",
    "@prisma/client": "4.3.1",
    "@tanstack/react-query": "4.3.9",
    "apollo-server-micro": "3.10.2",
    "classnames": "2.3.2",
    "daisyui": "2.24.0",
    "ethers": "5.7.1",
    "graphql": "^16.6.0",
    "micro": "^9.4.1",
    "next": "12.2.5",
    "next-auth": "4.10.3",
    "next-pwa": "^5.6.0",
    "next-superjson": "^0.0.4",
    "prisma": "4.3.1",
    "purify-ts": "1.3.0",
    "react": "18.2.0",
    "react-daisyui": "2.3.2",
    "react-dom": "18.2.0",
    "react-hook-form": "7.36.1",
    "react-markdown": "^8.0.3",
    "siwe": "1.1.6",
    "superjson": "^1.10.0",
    "wagmi": "0.6.6",
    "yup": "^0.32.11"
  },
