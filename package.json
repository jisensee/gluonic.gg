{
  "name": "gluonic",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "start": "next",
    "email:start": "cd react-email && npm start",
    "preview": "next build && next start",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "seed": "prisma db seed",
    "codegen": "prisma generate",
    "clean": "rm -rf .next",
    "ssl": "local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "lint": "next lint",
    "build": "next build",
    "vercel-build": "npm run check && npm run prisma:generate-prod && npm run prisma:migrate-prod && next build",
    "prisma:migrate-prod": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "prisma:generate-prod": "prisma generate --data-proxy",
    "prisma:generate-preview": "prisma generate"
  },
